{
  "name": "epifrin/rector-custom-rules",
  "description": "These are three rector rules to convert private method name, to convert local variable name to camel case and to replace double quotes with single quotes in string literals.",
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "rector/rector": "^1.0.0",
    "phpunit/phpunit": "^10.5.5",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.8"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Epifrin\\RectorCustomRules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Epifrin\\RectorCustomRules\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "epifrin",
      "email": "epifrin@users.noreply.github.com"
    }
  ],
  "minimum-stability": "stable"
}
